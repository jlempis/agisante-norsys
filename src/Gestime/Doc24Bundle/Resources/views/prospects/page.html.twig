{% extends '::base.html.twig' %}

{% block titrePage %}<i class="icon-ban-circle"></i> Prospects{% endblock %}

{% block title %}{{ action }}{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block includesjs %}
  {{ parent() }}
{% endblock %}

{% block body %}

<div class="row">
  <div class="col-lg-12">
    <div class="box">
      <div class="box-header">
        <span class="title"><i class="icon-user"></i>{{ action }}</span>
      </div>
      <div class="box-content">
        <form action="#" method="post" {{ form_enctype(form) }} class="form-horizontal fill-up validatable">
          <div class="padded">
            {% include 'GestimeDoc24Bundle:prospects:templates/fieldsListe.html.twig'  %}
          </div>
        </form>

      </div>
    </div>
  </div>
</div>
{% include 'GestimeDoc24Bundle:prospects:templates/datatable.html.twig'  %}

{% endblock %}
