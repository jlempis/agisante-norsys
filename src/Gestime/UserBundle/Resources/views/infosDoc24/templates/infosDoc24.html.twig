{% block _infosComplementaires_infosDoc24_widget %}
    {% spaceless %}
        {% if form.vars.value.photoPath != '' %}
            {% set photoPath = 'http://res.cloudinary.com/doc24/image/upload/w_140,h_160,c_fill,g_face/' ~ form.vars.value.photoPath ~ '.jpg' %}
        {% else %}
            {% set photoPath = asset('avatars/avatar_low.png') %}
        {% endif %}
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.photo, 'Avatar : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-4">
                        {{ form_widget(form.photo) }}
                        {{ form_errors(form.photo) }}
                    </div>
                </div>
                <div class="form-group">
                    {{ form_label(form.presentation, 'Présentation : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-7">
                        {{ form_widget(form.presentation) }}
                        {{ form_errors(form.presentation) }}
                    </div>
                </div>
            </div>
            <div class="col-lg-2">
                <div class="form-group">
                    <div class="col-lg-12">
                        <img src="{{ photoPath }}" alt="Photo du médecin"  width="140" height="160">
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.presentationLongue, 'Présentation Longue : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-7">
                        {{ form_widget(form.presentationLongue) }}
                        {{ form_errors(form.presentationLongue) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.langues, 'Langues: ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-9">
                        {{ form_widget(form.langues, { 'attr': { 'class':"select2-container select2-container-multi chzn-select", 'placeholder': "Cliquez pour ajouter une langue parlée" } }) }}
                        {{ form_errors(form.langues) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.specialitesMedicales, 'Spécialités: ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-9">
                        {{ form_widget(form.specialitesMedicales, { 'attr': { 'class':"select2-container select2-container-multi chzn-select", 'placeholder': "Cliquez pour ajouter une spécialité" } }) }}
                        {{ form_errors(form.specialitesMedicales) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.detailAcces, 'Détail accès : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-9">
                        {{ form_widget(form.detailAcces) }}
                        {{ form_errors(form.detailAcces) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.detailTelephone, 'Si urgence : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-9">
                        {{ form_widget(form.detailTelephone) }}
                        {{ form_errors(form.detailTelephone) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.infosPratiques, 'Informations pratiques : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-9">
                        {{ form_widget(form.infosPratiques) }}
                        {{ form_errors(form.infosPratiques) }}
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.transport, 'Moyens de transport: ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <button class="btn btn-green btn-transport">Ajouter un moyen de transport</button>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    <div class="col-lg-12">
                        {{ form_widget(form.transport) }}
                        {{ form_errors(form) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.carteVitale, 'Carte vitale: ', {'label_attr' : {'class' : 'control-label col-lg-3'}}) }}
                    <div class="col-lg-2">
                        {{ form_widget(form.carteVitale, {'attr': {'class' : "icheck"}}) }}
                        {{ form_errors(form.carteVitale) }}
                    </div>
                    {{ form_label(form.conventionnement, 'Secteur: ', {'label_attr' : {'class' : 'control-label col-lg-2'}}) }}
                    <div class="col-lg-1">
                        {{ form_widget(form.conventionnement) }}
                        {{ form_errors(form.conventionnement) }}
                    </div>
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.tarification, 'Tarification: ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <button class="btn btn-green btn-tarif">Ajouter une tarification</button>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    <div class="col-lg-12">
                    {{ form_widget(form.tarification) }}
                    {{ form_errors(form) }}
                </div>
            </div>
        </div>

        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.paiement_cb, 'Paiement par CB : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-1">
                        {{ form_widget(form.paiement_cb, { 'attr': {'class': "icheck"} }) }}
                        {{ form_errors(form.paiement_cb) }}
                    </div>
                    {{ form_label(form.site, 'Site : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-4">
                        {{ form_widget(form.site) }}
                        {{ form_errors(form.site) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.paiement_ch, 'Paiement par chèque : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-1">
                        {{ form_widget(form.paiement_ch, { 'attr': {'class': "icheck" }}) }}
                        {{ form_errors(form.paiement_ch) }}
                    </div>
                    {{ form_label(form.email, 'Email : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-4">
                        {{ form_widget(form.email) }}
                        {{ form_errors(form.email) }}
                    </div>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-lg-9">
                <div class="form-group">
                    {{ form_label(form.paiement_esp, 'Paiement par espèces : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-1">
                        {{ form_widget(form.paiement_esp, {'attr': {'class': "icheck"}}) }}
                        {{ form_errors(form.paiement_esp) }}
                    </div>
                    {{ form_label(form.tiers_payant, 'Tiers payant : ', {'label_attr': {'class': 'control-label col-lg-3'}}) }}
                    <div class="col-lg-4">
                        {{ form_widget(form.tiers_payant, {'attr': {'class': "icheck"}}) }}
                        {{ form_errors(form.tiers_payant) }}
                    </div>
                </div>
            </div>
        </div>
        {{ form_rest(form) }}
    {% endspaceless %}
{% endblock %}

{% block _infosComplementaires_horairesInternet_entry_widget %}
    {% spaceless %}
        <div class="rowHorairesInternet form-group" style="margin-left: -5px; margin-right: -5px; ">
            <div class="form-group">
                {{ form_label(form.jour, 'Jour:', {'label_attr': {'class': 'control-label col-lg-2'}} ) }}
                <div class="col-lg-2">
                    {{ form_widget(form.jour)}}
                </div>
                {{ form_label(form.debut, 'Début:', {'label_attr': {'class': 'control-label col-lg-1'}} ) }}
                <div class="col-lg-1">
                    {{ form_widget(form.debut)}}
                </div>
                {{ form_label(form.fin, 'Fin:', {'label_attr': {'class': 'control-label col-lg-1'}} ) }}
                <div class="col-lg-1">
                    {{ form_widget(form.fin)}}
                </div>
                {{ form_label(form.nbRdvMax, 'Nb Maxi Rdv:', {'label_attr': {'class': 'control-label col-lg-2'}} ) }}
                <div class="col-lg-1">
                    {{ form_widget(form.nbRdvMax) }}
                </div>
                <div class="col-lg-1 btnSupp"></div>
                <div class="error">
                    <br /><br />
                    {{ form_errors(form.debut) }}
                    {{ form_errors(form.fin) }}
                </div>
            </div>
        </div>
    {% endspaceless %}
{% endblock %}
